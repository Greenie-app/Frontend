    "lint:stylelint": "stylelint \"src/**/*.{vue,scss,css}\" --fix",
    "lint": "run-s lint:eslint lint:stylelint",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss}\"",
    "type-check": "vue-tsc --build",
    "docs:generate": "npx typedoc --out docs src",
    "ci:e2e:start-server": "yarn dev",
    "ci:e2e:run": "cypress run --record --key 83a81762-82f6-4993-a97a-5657287d9c0a",
